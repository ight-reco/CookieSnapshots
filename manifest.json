{
  "name": "Cookie Snapshots",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Save and restore cookies quickly! It's like behaves multiple login sessions.",
  "permissions": [
    "tabs",
    "\u003Call_urls\u003E", "cookies",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/util.js",
      "js/background.js"
    ]
  },
  "page_action": {
    "default_icon": "img/icon.png",
    "default_title": "Cookie Snapshots",
	"default_popup": "popup.html"
  },
  "icons": {
	"128": "img/icon.png"
  }
}

