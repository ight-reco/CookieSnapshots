{
  "name": "Cookie Snapshots",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "For switch login session speadly",
  "permissions": [
    "tabs",
    "\u003Call_urls\u003E", "cookies",
    "contextMenus",
    "unlimitedStorage",
    "notifications",
    "storage",
    "clipboardWrite"
  ],
  "background": {
    "scripts": [
      "js/util.js",
      "js/background.js"
    ]
  },
  "page_action": {
    "default_icon": "128.png",
    "default_title": "Cookie Snapshots",
	"default_popup": "popup.html"
  },
  "icons": {
    "128": "128.png"
  }
}

